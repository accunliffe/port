@import '//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.1.1/css/bootstrap.min.css';
@import url('http://fonts.googleapis.com/css?family=Raleway')
@font-face 
  font-family:'leagueG'
  src url('/fonts/LeagueGothic-Regular.otf')
@font-face 
  font-family:'Code'
  src url('/fonts/Code.otf')
@font-face 
  font-family:'MSans'
  src url('/fonts/M-Sans.ttf')
@font-face 
  font-family:'Arca'
  src url('/fonts/Arca-Heavy.ttf')
@media only screen and (max-width: 520px) {
     #nav { position: absolute; }
}

body
  overflow none
  background-color #212121
  background-size cover
  background-position left top


.container-fluid
  margin 0
  padding 0
  height auto
  text-align center

#nav.navbar
  background-color rgba(110,110,110,0.40)
  position fixed
  z-index 2
  min-width 100%
  text-align center
  border-bottom 1px solid white

img.logo2
  float right
  display inline-block  
  padding-top 1%
  padding-bottom 1%
  padding-right 1.25%
  height 3%
  width 3%
  min-width 62px

img.logo3
  float left
  display inline-block
  padding-top 7px
  padding-left 20px
  padding-bottom 7px
  height 36px
  width auto
  cursor pointer

h3.navtext1
  display inline-block
  font-family:'Arca'
  color white
  padding-top .75%
  padding-bottom 0
  letter-spacing 5px
  padding-right 2.5%
  text-align center
  font-size 120%

h3.navtext
  display inline-block
  padding-left 2%
  font-family:'MSans'
  color white
  margin-top 15px
  -webkit-font-smoothing antialiased
  letter-spacing 3px


.menubutton
  text-align left
  display inline-block
  float left
  padding-top 1%
  padding-bottom 1%

h3.menutext
  text-align left
  display inline-block
  font-family:'MSans'
  color white
  -webkit-font-smoothing antialiased
  letter-spacing 3px
  font-size 16px
  margin-top 12%

ul
  &.dropdown-menu
    background-color rgba(110,110,110,0.55)
    margin-right 1px
        


.dropdown-menu
  & li
    & a
      -webkit-font-smoothing antialiased      
      font-family:'MSans'
      color white
      font-size 120%


img.inline.ts
  max-height 600px
  max-width 100%
  


section
  &.module   
    &:last-child
      margin-bottom 0      
    h2
      margin-bottom 60px
      font-size 60px
      padding-bottom 30px
    p
      display inline-block
      font-family:'Raleway'
      margin-bottom 40px      
      font-weight 300
      font-size 18px
      height auto
      padding-bottom 40px
      line-height 30px
      text-align center
      &:last-child
        margin-bottom 0
      &.verticalLine
        width 1%
        height 600px
        border-right medium solid #D3D3D3
      &.left
        width 49.5%
        height auto
        float left
        padding-left 5px
      &.right
        width 49.5%
        height auto
        float right
        text-align left !important
        padding-left 20px
        padding-right 10px
      
    &.content
      padding 0 0
      height 1400px
      background-color white
    &.parallax
      height 60%
      background-position 50% 50%
      background-repeat no-repeat
      background-attachment fixed
      background-size cover
      &.top       
        padding-top 20%
        h1
          margin-bottom 55%
          line-height 100%
      h1
        color rgba(255, 255, 255, 0.8)
        font-size 48px
        line-height 600px
        font-weight 700
        text-align center
        text-transform uppercase
        text-shadow 0 0 10px rgba(0, 0, 0, 0.2)
        font-size 96px        
    &.parallax-1
      background-image url("/images/bridgebw.jpg")
    &.parallax-2
      background-image url("/images/lakeBW.jpg")
    &.parallax-3
      background-image url("/images/citybw.jpg")
    &.parallax-4
      background-image url("/images/FFBW.jpg")
    

nav.navbar#footer
  background-color rgba(110,110,110,0.75)
  height 100px
  margin 0 auto 0 auto
  position relative
  display block
  z-index 1
  text-align center

.shift-on-the-icon
  -webkit-transition background-image cubic-bezier(0.25, 0.46, 0.45, 0.94) 0.5s
  -moz-transition background-image cubic-bezier(0.25, 0.46, 0.45, 0.94) 0.5s
  -o-transition background-image cubic-bezier(0.25, 0.46, 0.45, 0.94) 0.5s
  transition background-image cubic-bezier(0.25, 0.46, 0.45, 0.94) 0.5s

.iconwrap
  display inline-block
  position relative
  vertical-align center      
  padding 5px 0 5px 15px
  float right
  font-family: 'MSans'
  -webkit-font-smoothing antialiased
  font-size 100%
  color white
  width auto
  height auto

p#float-l
  float left
  padding 17% 0 0 0

p#float-l2
  float left
  padding 1% 0 0 1%
  font-family: 'MSans'
  -webkit-font-smoothing antialiased
  font-size 60%
  color white

.linkedin-icon
  display inline-block
  margin 20px 30px 0 5px
  width 48px
  height 48px
  vertical-align center
  position relative
  background-image url("data:image/png;base64,")
  background-repeat no-repeat
  &:hover
    cursor pointer
    background-image url("data:image/png;base64,")